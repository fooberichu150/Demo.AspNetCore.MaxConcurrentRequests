{
	"Logging": {
		"LogLevel": {
			"Default": "Warning"
		}
	},
	"AllowedHosts": "*",
	"MaxConcurrentRequests": {
		"Enabled": true,
		"Limit": 1,
		"MaxQueueLength": 2,
		"MaxTimeInQueue": 15000,
		"LimitExceededPolicy": "FifoQueueDropTail",
		"ExcludePaths": [
			"/error",
			"/css",
			"/js",
			"/dist"
		]
	}
}
